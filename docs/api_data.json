  {
    "type": "post",
    "url": "/api/v1/user/register",
    "title": "Register a new user",
    "name": "RegisterUser",
    "group": "User",
    "description": "<p>Registers a new user with the provided details. Mandatory fields are strictly required. Optional fields are validated only if provided.</p>",
    "body": [
      {
        "group": "Body",
        "type": "String",
        "optional": false,
        "field": "mobileNumber",
        "description": "<p>User's mobile number (required, must match /^[6-9]\\d{9}$/)</p>"
      },
      {
        "group": "Body",
        "type": "String",
        "optional": false,
        "field": "fullName",
        "description": "<p>User's full name (required)</p>"
      },
      {
        "group": "Body",
        "type": "String",
        "optional": true,
        "field": "address",
        "description": "<p>User's address (optional, must not be empty if provided)</p>"
      },
      {
        "group": "Body",
        "type": "String",
        "optional": true,
        "field": "dob",
        "description": "<p>User's date of birth (optional, format: YYYY-MM-DD)</p>"
      },
      {
        "group": "Body",
        "type": "String",
        "allowedValues": [
          "\"male\"",
          "\"female\"",
          "\"others\""
        ],
        "optional": true,
        "field": "gender",
        "description": "<p>User's gender (optional, must be one of allowed values)</p>"
      },
      {
        "group": "Body",
        "type": "String",
        "size": "12",
        "optional": true,
        "field": "aadharNumber",
        "description": "<p>User's 12-digit Aadhar number (optional, must match /^\\d{12}$/)</p>"
      },
      {
        "group": "Body",
        "type": "File",
        "optional": true,
        "field": "profileImage",
        "description": "<p>User profile image (optional, jpg/png/jpeg)</p>"
      },
      {
        "group": "Body",
        "type": "String",
        "optional": true,
        "field": "panNumber",
        "description": "<p>User's PAN number (optional, must match /^[A-Z]{5}[0-9]{4}[A-Z]{1}$/)</p>"
      }
    ],
    "success": {
      "fields": {
        "201": [
          {
            "group": "201",
            "type": "Boolean",
            "optional": false,
            "field": "success",
            "description": "<p>Indicates if the registration was successful</p>"
          },
          {
            "group": "201",
            "type": "String",
            "optional": false,
            "field": "message",
            "description": "<p>Success message</p>"
          },
          {
            "group": "201",
            "type": "Object",
            "optional": false,
            "field": "data",
            "description": "<p>Registered user data</p>"
          }
        ]
      }
    },
    "error": {
      "fields": {
        "400": [
          {
            "group": "400",
            "type": "Object",
            "optional": false,
            "field": "ValidationError",
            "description": "<p>Validation failed for one or more fields</p>"
          }
        ],
        "500": [
          {
            "group": "500",
            "type": "Object",
            "optional": false,
            "field": "InternalServerError",
            "description": "<p>Unexpected server error</p>"
          }
        ]
      }
    },
    "sampleRequest": [
      {
        "url": "/api/v1/user/register"
      }
    ],
    "examples": [
      {
        "title": "Request-Example:",
        "content": "{\n  \"mobileNumber\": \"9876543210\",\n  \"fullName\": \"Arun Prakash\",\n  \"address\": \"123, Main Street, Chennai\",\n  \"dob\": \"1990-05-15\",\n  \"gender\": \"male\",\n  \"aadharNumber\": \"123456789012\",\n  \"panNumber\": \"ABCDE1234F\"\n}",
        "type": "json"
      }
    ],
    "version": "0.0.0",
    "filename": "src/Core/Api/UserController/UserController.js",
    "groupTitle": "User"
  }
]
